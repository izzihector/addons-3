<?xml version='1.0' encoding='utf-8'?>
<odoo>
    <data>
        <record id="renter_inh_history_view_form" model="ir.ui.view">
            <field name="name">renter.history.inherit.view.form</field>
            <field name="model">product.product</field>
            <field name="inherit_id" ref="property_rental_mgt_app.product_normal_form_view_inherit"/>
            <field name="arch" type="xml">
                <xpath expr="//field[@name='property_id']" position="after">
                    <field name="total_administrative_fees" />
                    <field name="warranty" />
                    <field name="club_fees" />
                    <field name="diesel_fees" />
                </xpath>
            </field>
        </record>
        
    </data>
</odoo>